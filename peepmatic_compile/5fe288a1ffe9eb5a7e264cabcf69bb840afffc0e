(=> (when (imul $x $C)
       (is-power-of-two $x $C)
     (is-power-of-two $x $C)
          (is-power-of-two $C)
      (is-power-of-two $x $C)
       (is-power-of-two $x $C)
      (is-power-of-two $x $c)
          (is-power-of-two $C)
      (is-power-of-two $x $C)
          (is-power-of-two $-o 