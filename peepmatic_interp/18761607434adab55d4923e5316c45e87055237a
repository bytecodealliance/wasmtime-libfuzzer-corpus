;; Replace with zero.
(=> (imul_imm 0 $x) 0)
(=> (band_imm 0 $$x) $x)
(=> (ishl_imm 0 $x) $x)
(=> (ushr_imm 0 $x) $x)
(=> (sshr_imm 0 $x) $x)

