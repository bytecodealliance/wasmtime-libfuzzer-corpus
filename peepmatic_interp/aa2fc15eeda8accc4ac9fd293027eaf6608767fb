(=> (imul $x (iconst $C)) (imul_imm $C $x))
(=> (imul (iconst $C) $x) (imul_imm $C $x))

;; Multor$y))
(=> (bor $y (bor $x $y)) (bor $x $y))
(=> (bor (bor $x $y) $X) (bor $x $y))
(=> (bor (bor $x $y) $y) (bor $x $y))

(=> (bor $x 0) $x)
(=> (bor $x $x) $x)

(=> (imul $x 0) $x)
(=> (sshr $x 0) $x)
