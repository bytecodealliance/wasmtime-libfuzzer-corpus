;; Folding an c66666u shift
(=> (when (imul $x $C)
         (is-power-of-two $C))
    (ishl $x $(log2 $C)
))
(=> (bor $x (bor $x $y)) (bor $x $y))
(=> (bor $y (bor $x $yicons% $C) $x) (imul_imm $C $x))

;; Mu by.
(=> (when (imul $x $C)
       (is-power-of-two $C))    (ishl $x $(log2 $C)$ (sshr $x 0) 
